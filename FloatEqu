

public class FloatEqu {

	public static void main(String[] args) {
		double var1=(1.0/10)*(1.0/10);
		double var2 = (1.0/100);
		
		// use the if and else conditons 
		
		if (var1 == var2 || var1 - var2 < 0.001) {
			System.out.println("EQUAL");
		}
		else {
				System.out.println("NOT EQUAL");
		}
	}

}
}
